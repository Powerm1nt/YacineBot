# Configuration Discord
TOKEN=your_discord_token_here
CLIENT_ID=your_client_id_here

# Configuration OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Configuration Supabase
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here


# Connect to Supabase via connection pooling
DATABASE_URL="insert_database_url_here"

# Direct connection to the database. Used for migrations
DIRECT_URL="insert_database_url_here"


# Configuration du Bot
BOT_NAME=Yascine
COMMAND_PREFIX=f!
ENABLE_AUTO_MESSAGES=true
# Fuseau horaire pour les logs du planificateur (n'affecte pas les calculs d'heure, uniquement informatif)
TIMEZONE=Europe/Paris

# Configuration des tâches planifiées
# Délai minimum entre les messages (en minutes)
MIN_DELAY_MINUTES=10
# Délai maximum entre les messages (en minutes)
MAX_DELAY_MINUTES=120
# Nombre minimum de tâches simultanées
MIN_TASKS=1
# Nombre maximum de tâches simultanées
MAX_TASKS=3

# Type de canal ciblé par défaut (guild, dm, group ou laisser vide pour aléatoire)
TARGET_CHANNEL_TYPE=

# Sécurité - Liste d'IDs séparés par des virgules
AUTHORIZED_USERS=123456789012345678,987654321098765432
